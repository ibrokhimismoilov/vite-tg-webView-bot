{
  "name": "vite-starter",
  "author": "Ibrokhim Ismoilov | i.ismoilov",
  "description": "React Vite Starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-dev": "env-cmd -f .env.development tsc && vite build",
    "start-production": "env-cmd -f .env.production vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "plop": "plop",
    "i18n": "i18next 'src/**/*.{ts,tsx}' [-oc]",
    "prettier": "prettier --ignore-path .gitignore --write \"/*.{ts,tsx,css,scss}\""
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "env-cmd": "10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-query-devtools": "^4.29.19",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/lodash": "^4.14.195",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "antd": "^5.6.4",
    "axios": "^1.4.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "i18next": "^23.2.8",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-parser": "^3.7.0",
    "lodash": "^4.17.21",
    "plop": "^3.1.2",
    "prettier": "^3.0.0",
    "react-countup": "^6.4.2",
    "react-hook-form": "^7.45.1",
    "react-i18next": "^13.0.1",
    "react-input-mask": "^2.0.4",
    "react-number-format": "^5.2.2",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "redux-logger": "^3.0.6",
    "sass": "^1.63.6",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-svgr": "^3.2.0",
    "yup": "^1.2.0"
  }
}
